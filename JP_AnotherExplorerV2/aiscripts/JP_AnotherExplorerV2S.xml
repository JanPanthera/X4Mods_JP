<?xml version="1.0" encoding="utf-8"?>

<aiscript name="JP_AnotherExplorerV2S" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="aiscripts.xsd">

  <order id="JP_AnotherExplorerV2S" name="{8888888, 497}AnotherExplorerV2 - {8888888, 499}{8888888, 498}{20001, 201}{8888888, 499}" category="navigation" infinite="true">
    <params>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <param name="SECTOR" text="{20001, 201}" comment="Sector" type="object" default="if global.$SectorAEV2? then global.$SectorAEV2 else this.sector">
        <input_param name="class" value="[class.sector]"/>
      </param>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <param name="GATES" text="{8888888, 410}" comment="Gates?" type="bool" default="if global.$GatesAEV2? then global.$GatesAEV2 else true"/>
      <param name="STATIONS" text="{8888888, 411}" comment="Stations?" type="bool" default="if global.$StationsAEV2? then global.$StationsAEV2 else true"/>
      <param name="ENEMY_STATIONS" text="- {8888888, 412}" comment="Enemy Stations?" type="bool" default="if global.$EnemyStationsAEV2? then global.$EnemyStationsAEV2 else false"/>
      <param name="FRIENDLY_STATIONS" text="- {8888888, 413}" comment="Friendly Stations?" type="bool" default="if global.$FriendlyStationsAEV2? then global.$FriendlyStationsAEV2 else true"/>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <param name="AUTO_FLEET" text="{8888888, 420}" comment="Automatic Fleet?" type="bool" default="if global.$AutoFleetAEV2? then global.$AutoFleetAEV2 else false"/>
      <param name="IS_AUTO_FLEET_LEADER" text="- {8888888, 421}" comment="Is Leader of automatic Fleet?" type="bool" default="false"/>
      <param name="COLOR_AUTOFLEET_NAME" text="- {8888888, 422}" comment="Color Automatic Fleet name?" type="bool" default="if global.$ColorAutoFleetNameAEV2? then global.$ColorAutoFleetNameAEV2 else true"/>
      <param name="AUTOFLEET_NAME_ALPHA" text="- {8888888, 470}" comment="Transparency" type="number" default="if $AutoFleetNameAlphaAEV2? then $AutoFleetNameAlphaAEV2 else 255">
        <input_param name="step" value="1"/>
        <input_param name="min" value="0"/>
        <input_param name="max" value="255"/>
      </param>
      <param name="AUTOFLEET_NAME_RED" text="- {8888888, 471}" comment="Red" type="number" default="if $AutoFleetNameRedAEV2? then $AutoFleetNameRedAEV2 else 3">
        <input_param name="step" value="1"/>
        <input_param name="min" value="0"/>
        <input_param name="max" value="255"/>
      </param>
      <param name="AUTOFLEET_NAME_GREEN" text="- {8888888, 472}" comment="Green" type="number" default="if $AutoFleetNameGreenAEV2? then $AutoFleetNameGreenAEV2 else 161">
        <input_param name="step" value="1"/>
        <input_param name="min" value="0"/>
        <input_param name="max" value="255"/>
      </param>
      <param name="AUTOFLEET_NAME_BLUE" text="- {8888888, 473}" comment="Blue" type="number" default="if $AutoFleetNameBlueAEV2? then $AutoFleetNameBlueAEV2 else 252">
        <input_param name="step" value="1"/>
        <input_param name="min" value="0"/>
        <input_param name="max" value="255"/>
      </param>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <param name="IDLE_TIME" text="{8888888, 480}" comment="Idle Time" type="time" default="if global.$IdleTimeAEV2? then global.$IdleTimeAEV2 else 600s">
        <input_param name="min" value="60s"/>
        <input_param name="max" value="3600s"/>
        <input_param name="step" value="10s"/>
      </param>
      <param name="IDLE_FOLLOW" text="{8888888, 481}" comment="Idle: Follow?" type="bool" default="if global.$IdleFollowAEV2? then global.$IdleFollowAEV2 else false"/>
      <param name="WHO_TO_FOLLOW" text="- {8888888, 482}" comment="Who to follow?" type="object" default="if global.$WhoToFollowAEV2? then global.$WhoToFollowAEV2 else null">
        <input_param name="class" value="[class.ship]"/>
        <input_param name="owner" value="this.ship.owner"/>
      </param>
      <param name="IDLE_DOCKING" text="{8888888, 483}" comment="Idle: Docking?" type="bool" default="if global.$IdleDockingAEV2? then global.$IdleDockingAEV2 else false"/>
      <param name="WHERE_TO_DOCK" text="- {8888888, 484}" comment="Where to dock?" type="object" default="if global.$WhereToDockAEV2? then global.$WhereToDockAEV2 else null">
        <input_param name="class" value="if this.ship.isclass.[class.ship_s, class.ship_xs] then [class.station, class.ship_m, class.ship_l, class.ship_xl] else [class.station, class.ship_l, class.ship_xl]"/>
      </param>
      <param name="IDLE_RANDOM_MOVEMENT" text="{8888888, 485}" comment="Idle: Movement?" type="bool" default="if global.$IdleRandomMovementAEV2? then global.$IdleRandomMovementAEV2 else false"/>
      <param name="IN_WHICH_SECTOR" text="- {8888888, 486}" comment="In which sector?" type="position" default="if global.$InWhichSectorAEV2? then global.$InWhichSectorAEV2 else [this.ship.sector, this.ship.position]">
        <input_param name="class" value="class.sector"/>
      </param>
      <param name="IDLE_MOVE_TO" text="{8888888, 487}" comment="Idle: Move to?" type="bool" default="if global.$IdleMoveToAEV2? then global.$IdleMoveToAEV2 else false"/>
      <param name="WHERE_TO_MOVE" text="- {8888888, 488}" comment="Where to move?" type="position" default="if global.$WhereToMoveAEV2? then global.$WhereToMoveAEV2 else [this.ship.sector, this.ship.position]">
        <input_param name="class" value="class.sector"/>
      </param>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <param name="ADD_ORDER_TAG" text="{8888888, 494}" comment="Add order tag?" type="bool" default="if global.$AddOrderTagAEV2? then global.$AddOrderTagAEV2 else true"/>
      <param name="SAVE_SETTINGS" text="{8888888, 495}" comment="SaveSettings" type="bool" default="false"/>
      <param name="DELETE_SETTINGS" text="{8888888, 496}" comment="DeleteSettings" type="bool" default="false"/>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <!-- DEBUG -->
      <param name="DEBUG" text="DebugText" type="number" default="if global.$DebugAEV2? then global.$DebugAEV2 else 0" advanced="true">
        <input_param name="min" value="0"/>
        <input_param name="max" value="100"/>
        <input_param name="step" value="100"/>
      </param>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    </params>
    <skill min="0"/>
    <requires>
      <match shiptype="shiptype.lasertower" negate="true"/>
    </requires>
    <location object="$_NearestUnknownObject" condition="@$_NearestUnknownObject.class != null"/>
  </order>

  <interrupts>
    <handler ref="SectorChangeHandler"/>
    <handler ref="TargetInvalidHandler"/>
    <handler ref="AttackHandler"/>
    <handler ref="MissileLockHandler"/>
    <handler ref="ScannedHandler"/>
    <handler ref="InspectedHandler"/>
    <handler ref="FoundAbandonedHandler"/>
    <handler ref="FoundLockboxHandler"/>
    <handler ref="ResupplyHandler"/>
    <handler ref="JobRemoveRequestHandler"/>
  </interrupts>

  <init>
    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <do_if value="$DEBUG gt 0">
      <set_value name="$DebugDirectory" exact="'JP_AnotherExplorerV2.logs'"/>
      <set_value name="$DebugFileName" exact="this.ship.idcode + '.JP_AnotherExplorerV2S.xml.log'"/>
    </do_if>
    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <set_value name="$Ship" exact="this.ship"/>
    <set_value name="$ExpMultiplier" exact="0.90"/>
    <set_value name="this.$CurrentSector" exact="$SECTOR"/>
    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <do_if value=" not global.$AnotherExplorerV2SectorBlackList?">
      <create_list name="global.$AnotherExplorerV2SectorBlackList"/>
    </do_if>
    <do_if value="not global.$AnotherExplorerV2Ships?">
      <create_list name="global.$AnotherExplorerV2Ships"/>
    </do_if>
    <append_to_list name="global.$AnotherExplorerV2Ships" exact="this.ship"/>
    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <do_if value="$ADD_ORDER_TAG">
      <set_value name="$_ShipName" exact="$Ship.knownname"/>
      <set_object_name object="this.ship" name="$_ShipName"/>
      <substitute_text source="$_ShipName" text="$_ShipName">
        <replace string="'\033#FFA95908# #AEV2-S\033X'" with="''"/>
      </substitute_text>
      <set_object_name object="this.ship" name="$_ShipName"/>
      <remove_value name="$_ShipName"/>
      <set_object_name object="this.ship" name="this.ship.knownname + '\033#FFA95908# #AEV2-S\033X'"/>
    </do_if>
    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <do_if value="$SAVE_SETTINGS">
      <do_if value="not global.$SectorAEV2? or @global.$SectorAEV2 != $SECTOR">
        <set_value name="global.$SectorAEV2" exact="$SECTOR"/>
      </do_if>
      <do_if value="not global.$GatesAEV2? or @global.$GatesAEV2 != $GATES">
        <set_value name="global.$GatesAEV2" exact="$GATES"/>
      </do_if>
      <do_if value="not global.$StationsAEV2? or @global.$StationsAEV2 != $STATIONS">
        <set_value name="global.$StationsAEV2" exact="$STATIONS"/>
      </do_if>
      <do_if value="not global.$EnemyStationsAEV2? or @global.$EnemyStationsAEV2 != $ENEMY_STATIONS">
        <set_value name="global.$EnemyStationsAEV2" exact="$ENEMY_STATIONS"/>
      </do_if>
      <do_if value="not global.$FriendlyStationsAEV2? or @global.$FriendlyStationsAEV2 != $FRIENDLY_STATIONS">
        <set_value name="global.$FriendlyStationsAEV2" exact="$FRIENDLY_STATIONS"/>
      </do_if>
      <do_if value="not global.$AddOrderTagAEV2? or @global.$AddOrderTagAEV2 != $ADD_ORDER_TAG">
        <set_value name="global.$AddOrderTagAEV2" exact="$ADD_ORDER_TAG"/>
      </do_if>
      <do_if value="not global.$AutoFleetAEV2? or @global.$AutoFleetAEV2 != $AUTO_FLEET">
        <set_value name="global.$AutoFleetAEV2" exact="$AUTO_FLEET"/>
      </do_if>
      <do_if value="not global.$ColorAutoFleetNameAEV2? or @global.$ColorAutoFleetNameAEV2 != $COLOR_AUTOFLEET_NAME">
        <set_value name="global.$ColorAutoFleetNameAEV2" exact="$COLOR_AUTOFLEET_NAME"/>
      </do_if>
      <do_if value="not global.$AutoFleetNameAlphaAEV2? or @global.$AutoFleetNameAlphaAEV2 != $AUTOFLEET_NAME_ALPHA">
        <set_value name="global.$AutoFleetNameAlphaAEV2" exact="$AUTOFLEET_NAME_ALPHA"/>
      </do_if>
      <do_if value="not global.$AutoFleetNameRedAEV2? or @global.$AutoFleetNameRedAEV2 != $AUTOFLEET_NAME_RED">
        <set_value name="global.$AutoFleetNameRedAEV2" exact="$AUTOFLEET_NAME_RED"/>
      </do_if>
      <do_if value="not global.$AutoFleetNameGreenAEV2? or @global.$AutoFleetNameGreenAEV2 != $AUTOFLEET_NAME_GREEN">
        <set_value name="global.$AutoFleetNameGreenAEV2" exact="$AUTOFLEET_NAME_GREEN"/>
      </do_if>
      <do_if value="not global.$AutoFleetNameBlueAEV2? or @global.$AutoFleetNameBlueAEV2 != $AUTOFLEET_NAME_BLUE">
        <set_value name="global.$AutoFleetNameBlueAEV2" exact="$AUTOFLEET_NAME_BLUE"/>
      </do_if>
      <do_if value="not global.$IdleTimeAEV2? or @global.$IdleTimeAEV2 != $IDLE_TIME">
        <set_value name="global.$IdleTimeAEV2" exact="$IDLE_TIME"/>
      </do_if>
      <do_if value="not global.$IdleFollowAEV2? or @global.$IdleFollowAEV2 != $IDLE_FOLLOW">
        <set_value name="global.$IdleFollowAEV2" exact="$IDLE_FOLLOW"/>
      </do_if>
      <do_if value="not global.$WhoToFollowAEV2? or @global.$WhoToFollowAEV2 != $WHO_TO_FOLLOW">
        <set_value name="global.$WhoToFollowAEV2" exact="$WHO_TO_FOLLOW"/>
      </do_if>
      <do_if value="not global.$IdleDockingAEV2? or @global.$IdleDockingAEV2 != $IDLE_DOCKING">
        <set_value name="global.$IdleDockingAEV2" exact="$IDLE_DOCKING"/>
      </do_if>
      <do_if value="not global.$WhereToDockAEV2? or @global.$WhereToDockAEV2 != $WHERE_TO_DOCK">
        <set_value name="global.$WhereToDockAEV2" exact="$WHERE_TO_DOCK"/>
      </do_if>
      <do_if value="not global.$IdleRandomMovementAEV2? or @global.$IdleRandomMovementAEV2 != $IDLE_RANDOM_MOVEMENT">
        <set_value name="global.$IdleRandomMovementAEV2" exact="$IDLE_RANDOM_MOVEMENT"/>
      </do_if>
      <do_if value="not global.$InWhichSectorAEV2? or @global.$InWhichSectorAEV2 != $IN_WHICH_SECTOR">
        <set_value name="global.$InWhichSectorAEV2" exact="$IN_WHICH_SECTOR"/>
      </do_if>
      <do_if value="not global.$IdleMoveToAEV2? or @global.$IdleMoveToAEV2 != $IDLE_MOVE_TO">
        <set_value name="global.$IdleMoveToAEV2" exact="$IDLE_MOVE_TO"/>
      </do_if>
      <do_if value="not global.$WhereToMoveAEV2? or @global.$WhereToMoveAEV2 != $WHERE_TO_MOVE">
        <set_value name="global.$WhereToMoveAEV2" exact="$WHERE_TO_MOVE"/>
      </do_if>
      <do_if value="not global.$DebugAEV2? or @global.$DebugAEV2 != $DEBUG">
        <set_value name="global.$DebugAEV2" exact="$DEBUG"/>
      </do_if>
    </do_if>
    <do_if value="$DELETE_SETTINGS">
      <remove_value name="global.$DebugAEV2"/>
      <remove_value name="global.$WhereToMoveAEV2"/>
      <remove_value name="global.$IdleMoveToAEV2"/>
      <remove_value name="global.$InWhichSectorAEV2"/>
      <remove_value name="global.$IdleRandomMovementAEV2"/>
      <remove_value name="global.$WhereToDockAEV2"/>
      <remove_value name="global.$IdleDockingAEV2"/>
      <remove_value name="global.$WhoToFollowAEV2"/>
      <remove_value name="global.$IdleFollowAEV2"/>
      <remove_value name="global.$IdleTimeAEV2"/>
      <remove_value name="global.$ColorAutoFleetNameAEV2"/>
      <remove_value name="global.$AutoFleetNameAlphaAEV2"/>
      <remove_value name="global.$AutoFleetNameRedAEV2"/>
      <remove_value name="global.$AutoFleetNameGreenAEV2"/>
      <remove_value name="global.$AutoFleetNameBlueAEV2"/>
      <remove_value name="global.$AutoFleetAEV2"/>
      <remove_value name="global.$AddOrderTagAEV2"/>
      <remove_value name="global.$FriendlyStationsAEV2"/>
      <remove_value name="global.$EnemyStationsAEV2"/>
      <remove_value name="global.$StationsAEV2"/>
      <remove_value name="global.$GatesAEV2"/>
      <remove_value name="global.$SectorAEV2"/>
      <!-- Cancel order because the settings are invalid.. because they were deleted -->
      <cancel_order order="this.ship.order"/>
    </do_if>
    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  </init>

  <attention min="unknown">
    <actions>

      <wait exact="1ms"/>
      <label name="START_LBL"/>

      <do_if value="$DEBUG gt 0">
        <debug_to_file append="false" directory="$DebugDirectory" name="$DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~'"/>
        <debug_to_file directory="$DebugDirectory" name="$DebugFileName" text="'[' + player.systemtime.{'%F - %X'} + '] ' + '~~ JP_AnotherExplorerV2S.xml ~ Started ~~' + '\n'"/>
      </do_if>

      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      
      <label name="PRE_SETTING_UP_LBL"/>
      <do_if value="$COLOR_AUTOFLEET_NAME">
        <run_script name="'JP_CreateColorString_AEV2'">
          <param name="ALPHA" value="$AUTOFLEET_NAME_ALPHA"/>
          <param name="RED" value="$AUTOFLEET_NAME_RED"/>
          <param name="GREEN" value="$AUTOFLEET_NAME_GREEN"/>
          <param name="BLUE" value="$AUTOFLEET_NAME_BLUE"/>
          <param name="DEBUG" value="$DEBUG"/>
          <save_retval name="COLOR_STRING" variable="$_ColorString"/>
        </run_script>
        <do_if value="not global.$AEV2_AFCS? or @global.$AEV2_AFCS != $_ColorString">
          <set_value name="global.$AEV2_AFCS" exact="$_ColorString"/>
        </do_if>
        <remove_value name="$_ColorString"/>
      </do_if>

      <label name="RESUME_LBL"/>

      <set_command command="command.explore"/>
      <set_command_action commandaction="commandaction.calculating"/>

      <run_script name="'JP_GetNearestUnknownObject_AEV2'">
        <param name="GATES" value="$GATES"/>
        <param name="STATIONS" value="$STATIONS"/>
        <param name="ENEMY_STATIONS" value="$ENEMY_STATIONS"/>
        <param name="FRIENDLY_STATIONS" value="$FRIENDLY_STATIONS"/>
        <param name="SECTOR" value="this.$CurrentSector"/>
        <param name="DEBUG" value="$DEBUG"/>
        <save_retval name="NEAREST_UNKNOWN_OBJECT" variable="$_NearestUnknownObject"/>
      </run_script>

      <do_if value="$_NearestUnknownObject == 'NoUnknownObjectFound'">
        <resume label="IDLE_RETURN_HOME_LBL"/>
      </do_if>

      <append_to_list name="global.$AnotherExplorerV2SectorBlackList" exact="this.$CurrentSector"/>

      <label name="MOVE_TO_SECTOR_LBL"/>
      <do_if value="$Ship.sector != this.$CurrentSector">
        <set_command command="command.move" param="this.$CurrentSector"/>
        <run_script name="'move.generic'">
          <param name="destination" value="this.$CurrentSector"/>
          <param name="endintargetzone" value="true"/>
          <param name="recallsubordinates" value="true"/>
        </run_script>
        <do_if value="this.zone.isclass.highway">
          <get_safe_pos result="$_SafePos" zone="$Ship.zone" object="$Ship" direction="quadrant.front" radius="$Ship.size" angle="45deg"/>
          <move_to object="$Ship" destination="$Ship.zone" uselocalhighways="false" finishonapproach="true" abortpath="true">
            <position value="$_SafePos"/>
          </move_to>
          <remove_value name="$_SafePos"/>
        </do_if>
        <wait exact="1s"/>
        <do_if value="$Ship.sector != this.$CurrentSector">
          <resume label="MOVE_TO_SECTOR_LBL"/>
        </do_if>
      </do_if>

      <label name="EXPLORE_OBJECT_LBL"/>
      <run_script name="'JP_ExploreObject'">
        <param name="OBJECT" value="$_NearestUnknownObject"/>
        <param name="DEBUG" value="$DEBUG"/>
      </run_script>
      <apply_experience entity="$Ship.pilot" experience="'JP_SHIP_OBJECT_DISCOVERED'" factor="$ExpMultiplier"/>
      <apply_experience object="$Ship" experience="'JP_SHIP_OBJECT_DISCOVERED'" role="entityrole.service" factor="$ExpMultiplier"/>

      <remove_from_list name="global.$AnotherExplorerV2SectorBlackList" exact="this.$CurrentSector"/>
      <remove_value name="$_NearestUnknownObject"/>

      <wait exact="1s"/>
      <resume label="RESUME_LBL"/>

      <label name="IDLE_RETURN_HOME_LBL"/>
      <create_order id="'JP_IdleReturnHome_AEV2'" object="$Ship">
        <param name="IDLE_TIME" value="$IDLE_TIME"/>
        <param name="IDLE_FOLLOW" value="$IDLE_FOLLOW"/>
        <param name="WHO_TO_FOLLOW" value="$WHO_TO_FOLLOW"/>
        <param name="IDLE_DOCKING" value="$IDLE_DOCKING"/>
        <param name="WHERE_TO_DOCK" value="$WHERE_TO_DOCK"/>
        <param name="IDLE_RANDOM_MOVEMENT" value="$IDLE_RANDOM_MOVEMENT"/>
        <param name="IN_WHICH_SECTOR" value="$IN_WHICH_SECTOR"/>
        <param name="IDLE_MOVE_TO" value="$IDLE_MOVE_TO"/>
        <param name="WHERE_TO_MOVE" value="$WHERE_TO_MOVE"/>
        <param name="DEBUG" value="$DEBUG"/>
      </create_order>
      <wait exact="1s"/>
      <resume label="RESUME_LBL"/>

      <label name="FINISH_LBL"/>

      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

      <do_if value="$DEBUG gt 0">
        <debug_to_file directory="$DebugDirectory" name="$DebugFileName" text="'[' + player.systemtime.{'%F - %X'} + '] ' + '~~ JP_AnotherExplorerV2S.xml ~ Finished ~~'"/>
        <debug_to_file directory="$DebugDirectory" name="$DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\n'"/>
      </do_if>

      <label name="CLEANUP_LBL"/>
      <do_if value="$ADD_ORDER_TAG">
        <set_value name="$_ShipName" exact="$Ship.knownname"/>
        <substitute_text source="$_ShipName" text="$_ShipName">
          <replace string="'\033#FFA95908# #AEV2-S\033X'" with="''"/>
        </substitute_text>
        <set_object_name object="this.ship" name="$_ShipName"/>
        <remove_value name="$_ShipName"/>
      </do_if>
      <do_if value="global.$AnotherExplorerV2SectorBlackList? and @this.$CurrentSector != null">
        <remove_from_list name="global.$AnotherExplorerV2SectorBlackList" exact="this.$CurrentSector"/>
      </do_if>
      <do_if value="global.$AnotherExplorerV2Ships?">
        <remove_from_list name="global.$AnotherExplorerV2Ships" exact="this.ship"/>
      </do_if>
      <do_if value="@global.$AnotherExplorerV2Ships.count == 0">
        <remove_value name="global.$AnotherExplorerV2Ships"/>
        <remove_value name="global.$AnotherExplorerV2SectorBlackList"/>
      </do_if>
      <do_if value="this.$CurrentSector?">
        <remove_value name="this.$CurrentSector"/>
      </do_if>

      <remove_value name="$Ship"/>
      <remove_value name="$ExpMultiplier"/>
      <do_if value="$DEBUG gt 0">
        <remove_value name="$DebugFileName"/>
        <remove_value name="$DebugDirectory"/>
      </do_if>

      <label name="END_LBL"/>
      <wait exact="1ms"/>
      <set_order_syncpoint_reached order="this.ship.order"/>
      <cancel_order order="this.ship.order" />

    </actions>
  </attention>

  <on_abort>
    <do_if value="$ADD_ORDER_TAG">
      <set_value name="$_ShipName" exact="$Ship.knownname"/>
      <substitute_text source="$_ShipName" text="$_ShipName">
        <replace string="'\033#FFA95908# #AEV2-S\033X'" with="''"/>
      </substitute_text>
      <set_object_name object="this.ship" name="$_ShipName"/>
      <remove_value name="$_ShipName"/>
    </do_if>
    <do_if value="global.$AnotherExplorerV2SectorBlackList? and @this.$CurrentSector != null">
      <remove_from_list name="global.$AnotherExplorerV2SectorBlackList" exact="this.$CurrentSector"/>
    </do_if>
    <do_if value="global.$AnotherExplorerV2Ships?">
      <remove_from_list name="global.$AnotherExplorerV2Ships" exact="this.ship"/>
    </do_if>
    <do_if value="@global.$AnotherExplorerV2Ships.count == 0">
      <remove_value name="global.$AnotherExplorerV2Ships"/>
      <remove_value name="global.$AnotherExplorerV2SectorBlackList"/>
    </do_if>
    <do_if value="this.$CurrentSector?">
      <remove_value name="this.$CurrentSector"/>
    </do_if>
  </on_abort>

</aiscript>