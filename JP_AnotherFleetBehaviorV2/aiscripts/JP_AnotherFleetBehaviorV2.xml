    </params>
    <skill min="0"/>
    <requires>
      <match shiptype="shiptype.lasertower" negate="true"/>
    </requires>
  </order>

  <interrupts>
    <handler ref="ScannedHandler"/>
    <handler ref="SectorChangeHandler"/>
    <handler ref="TargetInvalidHandler"/>
    <handler ref="AttackHandler"/>
    <handler ref="MissileLockHandler"/>
    <handler ref="ScannedHandler"/>
    <handler ref="InspectedHandler"/>
    <handler ref="FoundAbandonedHandler"/>
    <handler ref="FoundLockboxHandler"/>
    <handler ref="ResupplyHandler"/>
    <handler ref="JobRemoveRequestHandler"/>
  </interrupts>

  <init>
    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <do_if value="$DEBUG gt 0">
      <set_value name="$DebugDirectory" exact="'JP_AnotherFleetBehaviorV2.logs'"/>
      <set_value name="$DebugFileName" exact="this.ship.idcode + '.JP_AnotherFleetBehaviorV2.xml.log'"/>
    </do_if>
    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <set_value name="$Assignedcontrolled" exact="this.assignedcontrolled"/>
    <set_value name="$FleetCommander" exact="this.ship"/>
    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  </init>

  <attention min="unknown">
    <actions>

      <wait exact="1s"/>
      <label name="START_LBL"/>

      <do_if value="$DEBUG gt 0">
        <debug_to_file append="false" directory="$DebugDirectory" name="$DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~'"/>
        <debug_to_file directory="$DebugDirectory" name="$DebugFileName" text="'[' + player.systemtime.{'%F - %X'} + '] ' + '~~ JP_AnotherFleetBehaviorV2.xml ~ Started ~~' + '\n'"/>
      </do_if>

      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

      <do_for_each name="$_Subordinate" in="$Assignedcontrolled.allsubordinates">

        <do_if value="$_Subordinate.class == class.ship_s">

          <do_if value="@$_Subordinate.defaultorder.id == 'JP_AFBV2_AttackSmall'">
            <create_list name="$_Classes"/>
            <create_list name="$_Classess"/>
            <append_list_elements name="$_Classes" other="[class.ship_xs, class.ship_s, class.ship_m, class.ship_l, class.ship_xl, class.station]"/>
            <append_list_elements name="$_Classess" other="[$S_ATTACK_ATTACKS_XS, $S_ATTACK_ATTACKS_S, $S_ATTACK_ATTACKS_M, $S_ATTACK_ATTACKS_L, $S_ATTACK_ATTACKS_XL, $S_ATTACK_ATTACKS_STATIONS]"/>
            <create_list name="$_ClassesToAttack"/>
            <do_for_each name="$_Class" in="$_Classess" counter="$_it">
              <do_if value="$_Class">
                <append_to_list name="$_ClassesToAttack" exact="$_Classes.{$_it}"/>
              </do_if>
            </do_for_each>
            <remove_value name="$_Classess"/>
            <remove_value name="$_Classes"/>
            <edit_order_param order="$_Subordinate.defaultorder" param="'PRIMARY_TARGET'" value="$S_ATTACK_PRIMARY_TARGET"/>
            <edit_order_param order="$_Subordinate.defaultorder" param="'COMBAT_RANGE'" value="$S_ATTACK_RANGE"/>
            <edit_order_param order="$_Subordinate.defaultorder" param="'ATTACK_ENEMIES_IN_RANGE'" value="$S_ATTACK_ENEMIES_IN_RANGE"/>
            <edit_order_param order="$_Subordinate.defaultorder" param="'CLASSES_TO_ATTACK'" value="$_ClassesToAttack"/>
            <edit_order_param order="$_Subordinate.defaultorder" param="'STAY_DOCKED'" value="$S_ATTACK_STAY_DOCKED"/>
            <continue/>
          </do_if>

          <do_if value="@$_Subordinate.defaultorder.id == 'JP_AFBV2_DefenceSmall'">
            <create_list name="$_Classes"/>
            <create_list name="$_Classess"/>
            <append_list_elements name="$_Classes" other="[class.ship_xs, class.ship_s, class.ship_m, class.ship_l, class.ship_xl, class.station]"/>
            <append_list_elements name="$_Classess" other="[$S_DEFENCE_ATTACKS_XS, $S_DEFENCE_ATTACKS_S, $S_DEFENCE_ATTACKS_M, $S_DEFENCE_ATTACKS_L, $S_DEFENCE_ATTACKS_XL, $S_DEFENCE_ATTACKS_STATIONS]"/>
            <create_list name="$_ClassesToAttack"/>
            <do_for_each name="$_Class" in="$_Classess" counter="$_it">
              <do_if value="$_Class">
                <append_to_list name="$_ClassesToAttack" exact="$_Classes.{$_it}"/>
              </do_if>
            </do_for_each>
            <remove_value name="$_Classess"/>
            <remove_value name="$_Classes"/>
            <edit_order_param order="$_Subordinate.defaultorder" param="'COMBAT_RANGE'" value="$S_DEFENCE_RANGE"/>
            <edit_order_param order="$_Subordinate.defaultorder" param="'CLASSES_TO_ATTACK'" value="$_ClassesToAttack"/>
            <edit_order_param order="$_Subordinate.defaultorder" param="'STAY_DOCKED'" value="$S_DEFENCE_STAY_DOCKED"/>
            <continue/>
          </do_if>

          <do_if value="@$_Subordinate.defaultorder.id == 'JP_AFBV2_InterceptionSmall'">
            <create_list name="$_Classes"/>
            <create_list name="$_Classess"/>
            <append_list_elements name="$_Classes" other="[class.ship_xs, class.ship_s, class.ship_m, class.ship_l, class.ship_xl, class.station]"/>
            <append_list_elements name="$_Classess" other="[$S_INTERCEPT_ATTACKS_XS, $S_INTERCEPT_ATTACKS_S, $S_INTERCEPT_ATTACKS_M, $S_INTERCEPT_ATTACKS_L, $S_INTERCEPT_ATTACKS_XL, $S_INTERCEPT_ATTACKS_STATIONS]"/>
            <create_list name="$_ClassesToAttack"/>
            <do_for_each name="$_Class" in="$_Classess" counter="$_it">
              <do_if value="$_Class">
                <append_to_list name="$_ClassesToAttack" exact="$_Classes.{$_it}"/>
              </do_if>
            </do_for_each>
            <remove_value name="$_Classess"/>
            <remove_value name="$_Classes"/>
            <edit_order_param order="$_Subordinate.defaultorder" param="'COMBAT_RANGE'" value="$S_INTERCEPT_RANGE"/>
            <edit_order_param order="$_Subordinate.defaultorder" param="'CLASSES_TO_ATTACK'" value="$_ClassesToAttack"/>
            <edit_order_param order="$_Subordinate.defaultorder" param="'STAY_DOCKED'" value="$S_INTERCEPT_STAY_DOCKED"/>
            <continue/>
          </do_if>
        </do_if>
      </do_for_each>

      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

      <label name="RESUME_LBL"/>

      <do_for_each name="$_Subordinate" in="$Assignedcontrolled.allsubordinates">

        <do_if value="$_Subordinate.defaultorder.id == 'JP_AFBV2_Go_Repair' or $_Subordinate.isunit">
          <continue/>
        </do_if>

        <do_if value="$_Subordinate.class == class.ship_s">

          <do_if value="$_Subordinate.assignment == assignment.attack and $_Subordinate.defaultorder.id != 'JP_AFBV2_AttackSmall'">
            <create_list name="$_Classes"/>
            <create_list name="$_Classess"/>
            <append_list_elements name="$_Classes" other="[class.ship_xs, class.ship_s, class.ship_m, class.ship_l, class.ship_xl, class.station]"/>
            <append_list_elements name="$_Classess" other="[$S_ATTACK_ATTACKS_XS, $S_ATTACK_ATTACKS_S, $S_ATTACK_ATTACKS_M, $S_ATTACK_ATTACKS_L, $S_ATTACK_ATTACKS_XL, $S_ATTACK_ATTACKS_STATIONS]"/>
            <create_list name="$_ClassesToAttack"/>
            <do_for_each name="$_Class" in="$_Classess" counter="$_it">
              <do_if value="$_Class">
                <append_to_list name="$_ClassesToAttack" exact="$_Classes.{$_it}"/>
              </do_if>
            </do_for_each>
            <remove_value name="$_Classess"/>
            <remove_value name="$_Classes"/>
            <cancel_all_orders object="$_Subordinate"/>
            <create_order object="$_Subordinate" id="'JP_AFBV2_AttackSmall'" default="true">
              <param name="PRIMARY_TARGET" value="$S_ATTACK_PRIMARY_TARGET"/>
              <param name="COMBAT_RANGE" value="$S_ATTACK_RANGE"/>
              <param name="ATTACK_ENEMIES_IN_RANGE" value="$S_ATTACK_ENEMIES_IN_RANGE"/>
              <param name="CLASSES_TO_ATTACK" value="$_ClassesToAttack"/>
              <param name="STAY_DOCKED" value="$S_ATTACK_STAY_DOCKED"/>
              <param name="DEBUG" value="$DEBUG"/>
            </create_order>
          </do_if>

          <do_if value="$_Subordinate.assignment == assignment.defence and $_Subordinate.defaultorder.id != 'JP_AFBV2_DefenceSmall'">
            <create_list name="$_Classes"/>
            <create_list name="$_Classess"/>
            <append_list_elements name="$_Classes" other="[class.ship_xs, class.ship_s, class.ship_m, class.ship_l, class.ship_xl, class.station]"/>
            <append_list_elements name="$_Classess" other="[$S_DEFENCE_ATTACKS_XS, $S_DEFENCE_ATTACKS_S, $S_DEFENCE_ATTACKS_M, $S_DEFENCE_ATTACKS_L, $S_DEFENCE_ATTACKS_XL, $S_DEFENCE_ATTACKS_STATIONS]"/>
            <create_list name="$_ClassesToAttack"/>
            <do_for_each name="$_Class" in="$_Classess" counter="$_it">
              <do_if value="$_Class">
                <append_to_list name="$_ClassesToAttack" exact="$_Classes.{$_it}"/>
              </do_if>
            </do_for_each>
            <remove_value name="$_Classess"/>
            <remove_value name="$_Classes"/>
            <cancel_all_orders object="$_Subordinate"/>
            <create_order object="$_Subordinate" id="'JP_AFBV2_DefenceSmall'" default="true">
              <param name="COMBAT_RANGE" value="$S_DEFENCE_RANGE"/>
              <param name="CLASSES_TO_ATTACK" value="$_ClassesToAttack"/>
              <param name="STAY_DOCKED" value="$S_DEFENCE_STAY_DOCKED"/>
              <param name="DEBUG" value="$DEBUG"/>
            </create_order>
          </do_if>

          <do_if value="$_Subordinate.assignment == assignment.interception and $_Subordinate.defaultorder.id != 'JP_AFBV2_InterceptionSmall'">
            <create_list name="$_Classes"/>
            <create_list name="$_Classess"/>
            <append_list_elements name="$_Classes" other="[class.ship_xs, class.ship_s, class.ship_m, class.ship_l, class.ship_xl, class.station]"/>
            <append_list_elements name="$_Classess" other="[$S_INTERCEPT_ATTACKS_XS, $S_INTERCEPT_ATTACKS_S, $S_INTERCEPT_ATTACKS_M, $S_INTERCEPT_ATTACKS_L, $S_INTERCEPT_ATTACKS_XL, $S_INTERCEPT_ATTACKS_STATIONS]"/>
            <create_list name="$_ClassesToAttack"/>
            <do_for_each name="$_Class" in="$_Classess" counter="$_it">
              <do_if value="$_Class">
                <append_to_list name="$_ClassesToAttack" exact="$_Classes.{$_it}"/>
              </do_if>
            </do_for_each>
            <remove_value name="$_Classess"/>
            <remove_value name="$_Classes"/>
            <cancel_all_orders object="$_Subordinate"/>
            <create_order object="$_Subordinate" id="'JP_AFBV2_InterceptionSmall'" default="true">
              <param name="COMBAT_RANGE" value="$S_INTERCEPT_RANGE"/>
              <param name="CLASSES_TO_ATTACK" value="$_ClassesToAttack"/>
              <param name="STAY_DOCKED" value="$S_INTERCEPT_STAY_DOCKED"/>
              <param name="DEBUG" value="$DEBUG"/>
            </create_order>
          </do_if>

        </do_if>
      </do_for_each>

      <wait exact="1s"/>
      <resume label="RESUME_LBL"/>

      <label name="FINISH_LBL"/>

      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

      <do_if value="$DEBUG gt 0">
        <debug_to_file directory="$DebugDirectory" name="$DebugFileName" text="'[' + player.systemtime.{'%F - %X'} + '] ' + '~~ JP_AnotherFleetBehaviorV2.xml ~ Finished ~~'"/>
        <debug_to_file directory="$DebugDirectory" name="$DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\n'"/>
      </do_if>

      <label name="CLEANUP_LBL"/>
      <remove_value name="$FleetCommander"/>
      <remove_value name="$Assignedcontrolled"/>
      <do_if value="$DEBUG gt 0">
        <remove_value name="$DebugFileName"/>
        <remove_value name="$DebugDirectory"/>
      </do_if>

      <label name="END_LBL"/>
      <wait exact="1ms"/>
      <set_order_syncpoint_reached order="this.ship.order"/>
      <cancel_order order="this.ship.order" />

    </actions>
  </attention>

  <on_abort>
  </on_abort>

</aiscript>