      <param name="L_INTERCEPT_ATTACKS_XS" text="L_INTERCEPT_ATTACKS_XS" type="bool" default="false"/>
      <param name="L_INTERCEPT_ATTACKS_S" text="L_INTERCEPT_ATTACKS_S" type="bool" default="false"/>
      <param name="L_INTERCEPT_ATTACKS_M" text="L_INTERCEPT_ATTACKS_M" type="bool" default="true"/>
      <param name="L_INTERCEPT_ATTACKS_L" text="L_INTERCEPT_ATTACKS_L" type="bool" default="true"/>
      <param name="L_INTERCEPT_ATTACKS_XL" text="L_INTERCEPT_ATTACKS_XL" type="bool" default="true"/>
