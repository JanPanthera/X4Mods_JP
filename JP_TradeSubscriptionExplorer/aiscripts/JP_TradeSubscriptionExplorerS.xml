<?xml version="1.0" encoding="utf-8"?>

<aiscript name="JP_TradeSubscriptionExplorerS" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="aiscripts.xsd">

  <order id="JP_TradeSubscriptionExplorerS" name="{8888888, 1997}TradeSubscriptionExplorer - {8888888, 1999}{8888888, 1998}{20001, 201}{8888888, 1999}" category="navigation" infinite="true">
    <params>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <param name="SECTOR" text="{20001, 201}" comment="Sector" type="object" default="if global.this.$CurrentSectorTSE? then global.this.$CurrentSectorTSE else this.sector">
        <input_param name="class" value="[class.sector]"/>
      </param>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <param name="AUTO_FLEET" text="{8888888, 1910}" comment="Automatic Fleet?" type="bool" default="if global.$AutoFleetTSE? then global.$AutoFleetTSE else false"/>
      <param name="COLOR_AUTOFLEET_NAME" text="{8888888, 1911}" comment="Color Automatic Fleet name?" type="bool" default="if global.$ColorAutoFleetNameTSE? then global.$ColorAutoFleetNameTSE else true"/>
      <param name="AUTOFLEET_NAME_ALPHA" text="- {8888888, 1970}" comment="Transparency" type="number" default="if $AutoFleetNameAlphaTSE? then $AutoFleetNameAlphaTSE else 255">
        <input_param name="step" value="1"/>
        <input_param name="min" value="0"/>
        <input_param name="max" value="255"/>
      </param>
      <param name="AUTOFLEET_NAME_RED" text="- {8888888, 1971}" comment="Red" type="number" default="if $AutoFleetNameRedTSE? then $AutoFleetNameRedTSE else 3">
        <input_param name="step" value="1"/>
        <input_param name="min" value="0"/>
        <input_param name="max" value="255"/>
      </param>
      <param name="AUTOFLEET_NAME_GREEN" text="- {8888888, 1972}" comment="Green" type="number" default="if $AutoFleetNameGreenTSE? then $AutoFleetNameGreenTSE else 161">
        <input_param name="step" value="1"/>
        <input_param name="min" value="0"/>
        <input_param name="max" value="255"/>
      </param>
      <param name="AUTOFLEET_NAME_BLUE" text="- {8888888, 1973}" comment="Blue" type="number" default="if $AutoFleetNameBlueTSE? then $AutoFleetNameBlueTSE else 252">
        <input_param name="step" value="1"/>
        <input_param name="min" value="0"/>
        <input_param name="max" value="255"/>
      </param>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <param name="IDLE_TIME" text="{8888888, 1980}" comment="Idle Time" type="time" default="if global.$IdleTimeTSE? then global.$IdleTimeTSE else 600s">
        <input_param name="min" value="60s"/>
        <input_param name="max" value="3600s"/>
        <input_param name="step" value="10s"/>
      </param>
      <param name="IDLE_FOLLOW" text="{8888888, 1981}" comment="Idle: Follow?" type="bool" default="if global.$IdleFollowTSE? then global.$IdleFollowTSE else false"/>
      <param name="WHO_TO_FOLLOW" text="- {8888888, 1982}" comment="Who to follow?" type="object" default="if global.$WhoToFollowTSE? then global.$WhoToFollowTSE else null">
        <input_param name="class" value="[class.ship]"/>
        <input_param name="owner" value="this.ship.owner"/>
      </param>
      <param name="IDLE_DOCKING" text="{8888888, 1983}" comment="Idle: Docking?" type="bool" default="if global.$IdleDockingTSE? then global.$IdleDockingTSE else false"/>
      <param name="WHERE_TO_DOCK" text="- {8888888, 1984}" comment="Where to dock?" type="object" default="if global.$WhereToDockTSE? then global.$WhereToDockTSE? else null">
        <input_param name="class" value="if this.ship.isclass.[class.ship_s, class.ship_xs] then [class.station, class.ship_m, class.ship_l, class.ship_xl] else [class.station, class.ship_l, class.ship_xl]"/>
      </param>
      <param name="IDLE_RANDOM_MOVEMENT" text="{8888888, 1985}" comment="Idle: Movement?" type="bool" default="if global.$IdleRandomMovementTSE? then global.$IdleRandomMovementTSE else false"/>
      <param name="IN_WHICH_SECTOR" text="- {8888888, 1986}" comment="In which sector?" type="position" default="if global.$InWhichSectorTSE? then global.$InWhichSectorTSE else [this.ship.sector, this.ship.position]">
        <input_param name="class" value="class.sector"/>
      </param>
      <param name="IDLE_MOVE_TO" text="{8888888, 1987}" comment="Idle: Move to?" type="bool" default="if global.$IdleMoveToTSE? then global.$IdleMoveToTSE else false"/>
      <param name="WHERE_TO_MOVE" text="- {8888888, 1988}" comment="Where to move?" type="position" default="if global.$WhereToMoveTSE? then global.$WhereToMoveTSE else [this.ship.sector, this.ship.position]">
        <input_param name="class" value="class.sector"/>
      </param>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <param name="ADD_ORDER_TAG" text="{8888888, 1994}" comment="Add order tag?" type="bool" default="if global.$AddOrderTagTSE? then global.$AddOrderTagTSE else true"/>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <param name="SAVE_SETTINGS" text="{8888888, 1995}" comment="SaveSettings" type="bool" default="false"/>
      <param name="DELETE_SETTINGS" text="{8888888, 1996}" comment="DeleteSettings" type="bool" default="false"/>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <!-- DEBUG -->
      <param name="DEBUG" text="DebugText" type="number" default="if global.$DebugTSE? then global.$DebugTSE else 0" advanced="true">
        <input_param name="min" value="0"/>
        <input_param name="max" value="100"/>
        <input_param name="step" value="100"/>
      </param>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    </params>
    <skill min="0"/>
    <requires>
      <match shiptype="shiptype.lasertower" negate="true"/>
    </requires>
  </order>

  <interrupts>
    <handler ref="SectorChangeHandler"/>
    <handler ref="TargetInvalidHandler"/>
    <handler ref="AttackHandler"/>
    <handler ref="MissileLockHandler" />
    <handler ref="ScannedHandler"/>
    <handler ref="InspectedHandler"/>
    <handler ref="FoundAbandonedHandler"/>
    <handler ref="FoundLockboxHandler"/>
    <handler ref="ResupplyHandler"/>
    <handler ref="JobRemoveRequestHandler" />
  </interrupts>

  <init>
    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <set_value name="$DebugDirectory" exact="'JP_TradeSubscriptionExplorer.logs'"/>
    <set_value name="$DebugFileName" exact="this.ship.idcode + '.JP_TradeSubscriptionExplorerS.xml.log'"/>
    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <set_value name="$Ship" exact="this.ship"/>
    <set_value name="$ExpMultiplier" exact="0.90"/>
    <set_value name="this.$CurrentSector" exact="$SECTOR"/>
    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <do_if value="not global.$TradeSubscriptionExplorerSectorBlackList?">
      <create_list name="global.$TradeSubscriptionExplorerSectorBlackList"/>
    </do_if>
    <do_if value="not global.$TradeSubscriptionExplorerShips?">
      <create_list name="global.$TradeSubscriptionExplorerShips"/>
    </do_if>
    <do_if value="global.$TradeSubscriptionExplorerShips.count gt 0">
      <do_for_each name="$_Ship" in="global.$TradeSubscriptionExplorerShips">
        <do_if value="$_Ship == this.ship">
          <set_value name="$_WasInList" exact="true"/>
          <break/>
        </do_if>
      </do_for_each>
      <do_if value="not $_WasInList?">
        <append_to_list name="global.$TradeSubscriptionExplorerShips" exact="this.ship"/>
      </do_if>
    </do_if>
    <do_else>
      <append_to_list name="global.$TradeSubscriptionExplorerShips" exact="this.ship"/>
    </do_else>
    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <do_if value="not this.$OldName?">
      <set_value name="this.$OldName" exact="this.ship.knownname"/>
    </do_if>
    <do_if value="$ADD_ORDER_TAG">
      <set_object_name object="this.ship" name="this.ship.name + '\033#FFA2560B# #TSE-S\033X'"/>
    </do_if>
    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <do_if value="$SAVE_SETTINGS">
      <do_if value="not global.this.$CurrentSectorTSE? or @global.this.$CurrentSectorTSE != this.$CurrentSector">
        <set_value name="global.this.$CurrentSectorTSE" exact="this.$CurrentSector"/>
      </do_if>
      <do_if value="not global.$AddOrderTagTSE? or @global.$AddOrderTagTSE != $ADD_ORDER_TAG">
        <set_value name="global.$AddOrderTagTSE" exact="$ADD_ORDER_TAG"/>
      </do_if>
      <do_if value="not global.$AutoFleetTSE? or @global.$AutoFleetTSE != $AUTO_FLEET">
        <set_value name="global.$AutoFleetTSE" exact="$AUTO_FLEET"/>
      </do_if>
      <do_if value="not global.$ColorAutoFleetNameTSE? or @global.$ColorAutoFleetNameTSE != $COLOR_AUTOFLEET_NAME">
        <set_value name="global.$ColorAutoFleetNameTSE" exact="$COLOR_AUTOFLEET_NAME"/>
      </do_if>
      <do_if value="not global.$AutoFleetNameAlphaTSE? or @global.$AutoFleetNameAlphaTSE != $AUTOFLEET_NAME_ALPHA">
        <set_value name="global.$AutoFleetNameAlphaTSE" exact="$AUTOFLEET_NAME_ALPHA"/>
      </do_if>
      <do_if value="not global.$AutoFleetNameRedTSE? or @global.$AutoFleetNameRedTSE != $AUTOFLEET_NAME_RED">
        <set_value name="global.$AutoFleetNameRedTSE" exact="$AUTOFLEET_NAME_RED"/>
      </do_if>
      <do_if value="not global.$AutoFleetNameGreenTSE? or @global.$AutoFleetNameGreenTSE != $AUTOFLEET_NAME_GREEN">
        <set_value name="global.$AutoFleetNameGreenTSE" exact="$AUTOFLEET_NAME_GREEN"/>
      </do_if>
      <do_if value="not global.$AutoFleetNameBlueTSE? or @global.$AutoFleetNameBlueTSE != $AUTOFLEET_NAME_BLUE">
        <set_value name="global.$AutoFleetNameBlueTSE" exact="$AUTOFLEET_NAME_BLUE"/>
      </do_if>
      <do_if value="not global.$IdleTimeTSE? or @global.$IdleTimeTSE != $IDLE_TIME">
        <set_value name="global.$IdleTimeTSE" exact="$IDLE_TIME"/>
      </do_if>
      <do_if value="not global.$IdleFollowTSE? or @global.$IdleFollowTSE != $IDLE_FOLLOW">
        <set_value name="global.$IdleFollowTSE" exact="$IDLE_FOLLOW"/>
      </do_if>
      <do_if value="not global.$WhoToFollowTSE? or @global.$WhoToFollowTSE != $WHO_TO_FOLLOW">
        <set_value name="global.$WhoToFollowTSE" exact="$WHO_TO_FOLLOW"/>
      </do_if>
      <do_if value="not global.$IdleDockingTSE? or @global.$IdleDockingTSE != $IDLE_DOCKING">
        <set_value name="global.$IdleDockingTSE" exact="$IDLE_DOCKING"/>
      </do_if>
      <do_if value="not global.$WhereToDockTSE? or @global.$WhereToDockTSE != $WHERE_TO_DOCK">
        <set_value name="global.$WhereToDockTSE" exact="$WHERE_TO_DOCK"/>
      </do_if>
      <do_if value="not global.$IdleRandomMovementTSE? or @global.$IdleRandomMovementTSE != $IDLE_RANDOM_MOVEMENT">
        <set_value name="global.$IdleRandomMovementTSE" exact="$IDLE_RANDOM_MOVEMENT"/>
      </do_if>
      <do_if value="not global.$InWhichSectorTSE? or @global.$InWhichSectorTSE != $IN_WHICH_SECTOR">
        <set_value name="global.$InWhichSectorTSE" exact="$IN_WHICH_SECTOR"/>
      </do_if>
      <do_if value="not global.$IdleMoveToTSE? or @global.$IdleMoveToTSE != $IDLE_MOVE_TO">
        <set_value name="global.$IdleMoveToTSE" exact="$IDLE_MOVE_TO"/>
      </do_if>
      <do_if value="not global.$WhereToMoveTSE? or @global.$WhereToMoveTSE != $WHERE_TO_MOVE">
        <set_value name="global.$WhereToMoveTSE" exact="$WHERE_TO_MOVE"/>
      </do_if>
      <do_if value="not global.$DebugTSE? or @global.$DebugTSE != $DEBUG">
        <set_value name="global.$DebugTSE" exact="$DEBUG"/>
      </do_if>
    </do_if>
    <do_if value="$DELETE_SETTINGS">
      <remove_value name="global.$DebugTSE"/>
      <remove_value name="global.$WhereToMoveTSE"/>
      <remove_value name="global.$IdleMoveToTSE"/>
      <remove_value name="global.$InWhichSectorTSE"/>
      <remove_value name="global.$IdleRandomMovementTSE"/>
      <remove_value name="global.$WhereToDockTSE"/>
      <remove_value name="global.$IdleDockingTSE"/>
      <remove_value name="global.$WhoToFollowTSE"/>
      <remove_value name="global.$IdleFollowTSE"/>
      <remove_value name="global.$IdleTimeTSE"/>
      <remove_value name="global.$ColorAutoFleetNameTSE"/>
      <remove_value name="global.$AutoFleetNameAlphaTSE"/>
      <remove_value name="global.$AutoFleetNameRedTSE"/>
      <remove_value name="global.$AutoFleetNameGreenTSE"/>
      <remove_value name="global.$AutoFleetNameBlueTSE"/>
      <remove_value name="global.$AutoFleetTSE"/>
      <remove_value name="global.$AddOrderTagTSE"/>
      <remove_value name="global.this.$CurrentSectorTSE"/>
      <!-- cancel order because the settings are invalid now -->
      <cancel_order order="this.ship.order"/>
    </do_if>
    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  </init>

  <attention min="unknown">
    <actions>

      <wait exact="1ms"/>
      <label name="INIT"/>

      <do_if value="$DEBUG gt 0">
        <debug_to_file append="false" directory="$DebugDirectory" name="$DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~'"/>
        <debug_to_file directory="$DebugDirectory" name="$DebugFileName" text="'[' + player.systemtime.{'%F - %X'} + '] ' + '~~ JP_TradeSubscriptionExplorerS.xml ~ Started ~~' + '\n'"/>
      </do_if>

      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

      <label name="PRE_SETTING_UP"/>
      <do_if value="$COLOR_AUTOFLEET_NAME and $Ship == @global.$TradeSubscriptionExplorerFleetLeader or not @global.$TradeSubscriptionExplorerAutoFleetNameColorString">
        <run_script name="'JP_CreateColorString_TSE'">
          <param name="ALPHA" value="$AUTOFLEET_NAME_ALPHA"/>
          <param name="RED" value="$AUTOFLEET_NAME_RED"/>
          <param name="GREEN" value="$AUTOFLEET_NAME_GREEN"/>
          <param name="BLUE" value="$AUTOFLEET_NAME_BLUE"/>
          <param name="DEBUG" value="$DEBUG"/>
          <save_retval name="COLOR_STRING" variable="global.$TradeSubscriptionExplorerAutoFleetNameColorString"/>
        </run_script>
      </do_if>

      <label name="RESUME"/>

      <set_command command="command.explore"/>
      <set_command_action commandaction="commandaction.calculating"/>

      <run_script name="'JP_GetNearestStationWithExpiredTradeSubscription'">
        <param name="SECTOR" value="this.$CurrentSector"/>
        <param name="DEBUG" value="$DEBUG"/>
        <save_retval name="NEAREST_STATION" variable="$NearestStation"/>
      </run_script>

      <do_if value="$NearestStation == 'NoStationWithExpiredTradeSubscriptionFound'">
        <run_script name="'JP_IdleReturnHome_TSE'">
          <param name="IDLE_FOLLOW" value="$IDLE_FOLLOW"/>
          <param name="WHO_TO_FOLLOW" value="$WHO_TO_FOLLOW"/>
          <param name="IDLE_DOCKING" value="$IDLE_DOCKING"/>
          <param name="WHERE_TO_DOCK" value="$WHERE_TO_DOCK"/>
          <param name="IDLE_RANDOM_MOVEMENT" value="$IDLE_RANDOM_MOVEMENT"/>
          <param name="IN_WHICH_SECTOR" value="$IN_WHICH_SECTOR"/>
          <param name="IDLE_MOVE_TO" value="$IDLE_MOVE_TO"/>
          <param name="WHERE_TO_MOVE" value="$WHERE_TO_MOVE"/>
          <param name="IDLE_TIME" value="$IDLE_TIME"/>
          <param name="DEBUG" value="$DEBUG"/>
        </run_script>
        <set_object_name object="this.ship" name="if $ADD_ORDER_TAG then this.$OldName + '\033#FFA95908# #TSE-G\033X' else this.$OldName"/>
        <resume label="RESUME"/>
      </do_if>

      <append_to_list name="global.$TradeSubscriptionExplorerSectorBlackList" exact="this.$CurrentSector"/>

      <label name="MOVE_TO_SECTOR"/>
      <do_if value="$Ship.sector != this.$CurrentSector">
        <set_command command="command.move" param="this.$CurrentSector"/>
        <run_script name="'move.generic'">
          <param name="destination" value="this.$CurrentSector"/>
          <param name="endintargetzone" value="true"/>
          <param name="recallsubordinates" value="true"/>
        </run_script>
        <wait exact="1ms"/>
        <do_if value="$Ship.sector != this.$CurrentSector">
          <resume label="MOVE_TO_SECTOR"/>
        </do_if>
      </do_if>

      <label name="UPDATE_TRADE_SUBSCRIPTION"/>
      <run_script name="'JP_ExploreTradeSubscription'">
        <param name="STATION" value="$NearestStation"/>
        <param name="DEBUG" value="$DEBUG"/>
      </run_script>
      <apply_experience entity="$Ship.pilot" experience="'JP_SHIP_TRADESUBSCRIPTION_UPDATED'" factor="$ExpMultiplier"/>
      <apply_experience object="$Ship" experience="'JP_SHIP_TRADESUBSCRIPTION_UPDATED'" role="entityrole.service" factor="$ExpMultiplier"/>

      <do_if value="global.$TradeSubscriptionExplorerSectorBlackList.count gt 0">
        <do_for_each name="$_Sector" in="global.$TradeSubscriptionExplorerSectorBlackList">
          <do_if value="$_Sector == this.$CurrentSector">
            <set_value name="$_WasInList" exact="true"/>
            <break/>
          </do_if>
        </do_for_each>
        <do_if value="$_WasInList?">
          <remove_from_list name="global.$TradeSubscriptionExplorerSectorBlackList" exact="this.$CurrentSector"/>
        </do_if>
      </do_if>
      <remove_value name="$NearestStation"/>

      <wait exact="1s"/>
      <resume label="RESUME"/>

      <label name="FINISH"/>

      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

      <do_if value="$DEBUG gt 0">
        <debug_to_file directory="$DebugDirectory" name="$DebugFileName" text="'[' + player.systemtime.{'%F - %X'} + '] ' + '~~ JP_TradeSubscriptionExplorerS.xml ~ Finished ~~'"/>
        <debug_to_file directory="$DebugDirectory" name="$DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\n'"/>
      </do_if>

      <label name="CLEANUP"/>
      <do_if value="$ADD_ORDER_TAG and this.$OldName?">
        <set_object_name object="this.ship" name="this.$OldName"/>
      </do_if>
      <do_if value="this.ship == @global.$TradeSubscriptionExplorerFleetLeader">
        <do_if value="global.$TradeSubscriptionExplorerAutoFleetNameColorString?">
          <remove_value name="global.$TradeSubscriptionExplorerAutoFleetNameColorString"/>
        </do_if>
      </do_if>
      <do_if value="global.$TradeSubscriptionExplorerSectorBlackList? and this.$CurrentSector?">
        <do_for_each name="$_Sector" in="global.$TradeSubscriptionExplorerSectorBlackList">
          <do_if value="$_Sector == this.$CurrentSector">
            <set_value name="$_WasInList" exact="true"/>
            <break/>
          </do_if>
        </do_for_each>
        <do_if value="$_WasInList?">
          <remove_from_list name="global.$TradeSubscriptionExplorerSectorBlackList" exact="this.$CurrentSector"/>
          <remove_value name="$_WasInList"/>
        </do_if>
      </do_if>
      <do_if value="global.$TradeSubscriptionExplorerShips?">
        <do_for_each name="$_Ship" in="global.$TradeSubscriptionExplorerShips">
          <do_if value="$_Ship == this.ship">
            <set_value name="$_WasInList" exact="true"/>
            <break/>
          </do_if>
        </do_for_each>
        <do_if value="$_WasInList?">
          <remove_from_list name="global.$TradeSubscriptionExplorerShips" exact="this.ship"/>
          <remove_value name="$_WasInList"/>
        </do_if>
      </do_if>
      <do_if value="@global.$TradeSubscriptionExplorerShips.count le 0">
        <do_if value ="global.$TradeSubscriptionExplorerSectorBlackList?">
          <remove_value name="global.$TradeSubscriptionExplorerSectorBlackList"/>
        </do_if>
        <remove_value name="global.$TradeSubscriptionExplorerShips"/>
      </do_if>
      <do_if value="this.$OldName?">
        <remove_value name="this.$OldName"/>
      </do_if>
      <do_if value="this.$CurrentSector?">
        <remove_value name="this.$CurrentSector"/>
      </do_if>

      <remove_value name="$Ship"/>
      <remove_value name="$ExpMultiplier"/>
      <remove_value name="$DebugFileName"/>
      <remove_value name="$DebugDirectory"/>

      <label name="END"/>
      <wait exact="1ms"/>
      <set_order_syncpoint_reached order="this.ship.order"/>
      <cancel_order order="this.ship.order" />

    </actions>
  </attention>

  <on_abort>
    <do_if value="$ADD_ORDER_TAG and this.$OldName?">
      <set_object_name object="this.ship" name="this.$OldName"/>
    </do_if>
    <do_if value="this.ship == @global.$TradeSubscriptionExplorerFleetLeader">
      <do_if value="global.$TradeSubscriptionExplorerAutoFleetNameColorString?">
        <remove_value name="global.$TradeSubscriptionExplorerAutoFleetNameColorString"/>
      </do_if>
    </do_if>
    <do_if value="global.$TradeSubscriptionExplorerSectorBlackList? and this.$CurrentSector?">
      <do_for_each name="$_Sector" in="global.$TradeSubscriptionExplorerSectorBlackList">
        <do_if value="$_Sector == this.$CurrentSector">
          <set_value name="$_WasInList" exact="true"/>
          <break/>
        </do_if>
      </do_for_each>
      <do_if value="$_WasInList?">
        <remove_from_list name="global.$TradeSubscriptionExplorerSectorBlackList" exact="this.$CurrentSector"/>
        <remove_value name="$_WasInList"/>
      </do_if>
    </do_if>
    <do_if value="global.$TradeSubscriptionExplorerShips?">
      <do_for_each name="$_Ship" in="global.$TradeSubscriptionExplorerShips">
        <do_if value="$_Ship == this.ship">
          <set_value name="$_WasInList" exact="true"/>
          <break/>
        </do_if>
      </do_for_each>
      <do_if value="$_WasInList?">
        <remove_from_list name="global.$TradeSubscriptionExplorerShips" exact="this.ship"/>
        <remove_value name="$_WasInList"/>
      </do_if>
    </do_if>
    <do_if value="@global.$TradeSubscriptionExplorerShips.count le 0">
      <do_if value ="global.$TradeSubscriptionExplorerSectorBlackList?">
        <remove_value name="global.$TradeSubscriptionExplorerSectorBlackList"/>
      </do_if>
      <remove_value name="global.$TradeSubscriptionExplorerShips"/>
    </do_if>
    <do_if value="this.$OldName?">
      <remove_value name="this.$OldName"/>
    </do_if>
    <do_if value="this.$CurrentSector?">
      <remove_value name="this.$CurrentSector"/>
    </do_if>
  </on_abort>

</aiscript>
